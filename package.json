{
  "name": "globimp",
  "version": "1.0.45",
  "description": "Access node_modules without importing them",
  "contributors": [
    "Robin Lebhar <robin.lebhar.chill.studio@gmail.com>"
  ],
  "main": "index.js",
  "bin": {
    "globimp": "./bin/index.js"
  },
  "preferGlobal": true,
  "scripts": {
    "build": "npx tsc",
    "start": "npx tsc && node ./bin/index.js",
    "publish-npm": "tsc && npm publish --access public",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "Kakouloukiam",
  "license": "MIT",
  "keywords": [
    "global import",
    "global imports",
    "import",
    "imports",
    "import module",
    "global modules",
    "global",
    "module imports"
  ],
  "dependencies": {
    "typescript": "^4.7.4"
  },
  "devDependencies": {
    "@types/node": "^17.0.31"
  }
}
